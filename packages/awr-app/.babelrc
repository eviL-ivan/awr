{
  "presets": ["next/babel", "react", "react-optimize"],
  "plugins": [
    "transform-decorators-legacy",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "decorators": "./core/decorators",
          "services": "./core/services",
          "utils": "./core/utils",
          "theme": "./core/theme",
          "store": "./store"
        }
      }
    ],
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ]
  ]
}
